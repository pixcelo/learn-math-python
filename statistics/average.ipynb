   ],
   "source": [
    "# 期待値\n",
    "# 確率と値の積の総和により計算することができる\n",
    "p = np.array([0.8, 0.15, 0.05]) # 確率\n",
    "x = np.array([100, 500, 1000]) # 値\n",
    "\n",
    "np.sum(p*x)"
   ]
  },
