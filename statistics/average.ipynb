   ],
   "source": [
    "#　平均値\n",
    "import numpy as np\n",
    "\n",
    "x = np.array([55,45,60,40])\n",
    "\n",
    "np.average(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "data": {
